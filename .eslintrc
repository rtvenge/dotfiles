{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": 0,
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 1
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "jQuery": true
  }
}
